{
  "name": "masm",
  "displayName": "MASM",
  "description": "Mindustry Assembly",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "masm",
      "aliases": ["MASM", "masm"],
      "extensions": [".masm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "masm",
      "scopeName": "source.masm",
      "path": "./syntaxes/masm.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "masm",
        "path": "./snippets/masm-snippets.json"
      }
    ]
  }
}
